# Override the resource management policies
apiVersion: iam.cnrm.cloud.google.com/v1beta1
  kind: IAMAuditConfig
  metadata:
    name: iam-auditconfig-project
    namespace: project-id # kpt-set: ${project-id}
  spec:
    service: allServices
    auditLogConfigs:
      - logType: DATA_WRITE
      - logType: DATA_READ
      - logType: ADMIN_READ
    resourceRef:
      apiVersion: resourcemanager.cnrm.cloud.google.com/v1beta1
      kind: Project
      external: projects/project-id # kpt-set: projects/${project-id}
 